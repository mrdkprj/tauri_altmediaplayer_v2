{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "context": "local",
  "windows": [
    "Player",
    "Playlist",
    "ContextMenu"
  ],
  "permissions": [
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "path:default",
    "event:default",
    "window:default",
    "window:allow-start-dragging",
    "window:allow-close",
    "window:allow-show",
    "window:allow-hide",
    "window:allow-set-title",
    "window:allow-maximize",
    "window:allow-unmaximize",
    "window:allow-minimize",
    "window:allow-unminimize",
    "window:allow-toggle-maximize",
    "window:allow-set-fullscreen",
    "window:allow-set-focus",
    "window:allow-set-position",
    "window:allow-set-size",
    "fs:read-all",
    "fs:allow-remove",
    "fs:allow-exists",
    "shell:allow-execute",
    "clipboard-manager:allow-write",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "webview:allow-internal-toggle-devtools",
    {
      "identifier": "fs:scope",
      "allow": [{"path":"**"}]
    },
    {
      "identifier": "fs:allow-rename",
      "allow": [{"path":"**"}]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": [
            "/e,/select,",
            {
              "validator": "\\S+"
            }
          ],
          "cmd": "explorer",
          "name": "run-explorer",
          "sidecar": false
        },
        {
          "args": [
            {
              "validator": "\\S+"
            }
          ],
          "cmd": "",
          "name": "binaries/ffmpeg",
          "sidecar": true
        },
        {
          "args": [
            {
              "validator": "\\S+"
            }
          ],
          "cmd": "",
          "name": "binaries/ffprobe",
          "sidecar": true
        }
      ]
    }
  ],
  "platforms": [
    "linux",
    "macOS",
    "windows",
    "android",
    "iOS"
  ]
}